{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Template project for react with express",
  "scripts": {
    "build:client": "webpack --env.NODE_ENV=production --mode production",
    "build:server": "babel -d ./dist_server ./src/server",
    "dev:client": "webpack-dev-server --env.NODE_ENV=local --mode development",
    "dev:server": "nodemon --exec babel-node --inspect --watch src/server src/server",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "prestart": "npm run build:client && npm run build:server",
    "start": "node dist_server/index.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/runtime": "^7.15.4",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.12",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.3.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  }
}